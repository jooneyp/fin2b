<div class="container">
    <div class="form-group">
        <div class="row">
            <div class="col-sm-12 text-center">
                <h1>Fibonacci Calculator</h1>
            </div>
        </div>
        <h3 for="n">Input N:</h3>
        <input type="text" class="form-control" id="n">
        <br>
        <input type="submit" class="col-xs-12 btn btn-primary btn-load btn-lg" data-loading-text="Calculating..." value="Calculate!" id="calcFibonacci">
    </div>
    <div class="form-group">
        <br><br>
        <h3>Result</h3>
        <h2 for="result" id="result"></h2>
    </div>
</div>
<script src="/js/fibonacci.js" charset="utf-8"></script>
